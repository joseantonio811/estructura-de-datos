#include "stdafx.h"
#include <iostream>
#include <conio.h>
using namespace std;
int main () {
	int n,c,i;
	cout<<"ingrese un numero"<<endl;
	cin>>n;
	c=0;
	for (i=1; i<=n; i++){
		if (n%i==0) {
			c=c+1;
		}
	}
	if (c==2){
		cout<<"es primo"<<endl;
	}else{
		cout<<"no es primo"<<endl;
	}
	getch ();
	return (0);
